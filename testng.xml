<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Facebook POM framework">
  
  <listeners>
  <listener class-name="com.qa.extentreportlistner.ExtentReporterNG"/>
  <listener class-name="com.qa.screenshot.Listener"/>
