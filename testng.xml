      <class name="com.qa.testcases.SignupTestcase"/>
      <class name="com.qa.testcases.HomePageTest"/>
      <class name="com.qa.testcases.LoginTestcase"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
